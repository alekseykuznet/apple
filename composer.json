{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.16",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-httpclient": "2.0.11",
        "kartik-v/yii2-widget-select2": "^2.1",
        "yiisoft/yii2-swiftmailer": "^2.1",
        "kak/panel": "*",
        "kartik-v/yii2-date-range": "^1.7",
        "kak/fieldset": "dev-master",
        "mranger/yii2-ckeditor": "^1.2",
        "ckeditor/ckeditor": "dev-full/4.7.x",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-sortable": "@dev",
        "ext-curl": "*",
        "nesbot/carbon": "^2.25",
        "ext-zlib": "*",
        "ext-json": "*",
        "khr/php-mcurl-client": "~3.0",
        "geoip2/geoip2": "~2.0",
        "piwik/device-detector" : "3.9.*",
        "mobiledetect/mobiledetectlib": "^2.8",
        "kak/grid": ">=1.0.4",
        "kartik-v/yii2-grid": "@dev"
    },
    "require-dev": {
        "codeception/module-filesystem": "^1.0.0",
        "codeception/module-yii2": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
